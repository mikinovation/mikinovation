{
  "name": "@mikinovation/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:mock": "NUXT_PUBLIC_API_MOCK=true nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck",
    "test:unit": "vitest",
    "codegen": "openapi-typescript https://raw.githubusercontent.com/github/rest-api-description/refs/heads/main/descriptions/api.github.com/api.github.com.json -o types/github-api.ts"
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@sentry/nuxt": "9.36.0",
    "nuxt": "3.17.6",
    "openapi-fetch": "0.14.0",
    "vue": "3.5.17",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.5.2",
    "@nuxt/test-utils": "3.19.1",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.30.1",
    "happy-dom": "18.0.1",
    "msw": "2.10.4",
    "nuxt-mcp": "0.2.3",
    "openapi-typescript": "7.8.0",
    "playwright-core": "1.53.2",
    "sass": "1.89.2",
    "typescript": "5.8.3",
    "vite-plugin-mcp": "0.2.3",
    "vitest": "3.2.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
