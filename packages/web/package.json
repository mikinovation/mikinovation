{
  "name": "@mikinovation/web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:mock": "NUXT_PUBLIC_API_MOCK=true nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck",
    "codegen": "openapi-typescript https://raw.githubusercontent.com/github/rest-api-description/refs/heads/main/descriptions/api.github.com/api.github.com.json -o types/github-api.ts"
  },
  "packageManager": "pnpm@10.8.0",
  "dependencies": {
    "@nuxt/eslint": "1.3.0",
    "@nuxt/test-utils": "3.17.2",
    "nuxt": "3.16.2",
    "openapi-fetch": "0.13.5",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "eslint": "9.24.0",
    "msw": "2.7.4",
    "openapi-typescript": "7.6.1",
    "sass": "1.86.3",
    "typescript": "5.8.3"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
